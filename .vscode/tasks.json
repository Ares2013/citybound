{
    "version": "2.0.0",
    "command": "cargo",
    "isShellCommand": true,
    "showOutput": "always",
    "suppressTaskName": true,
    "tasks": [
        {
            "taskName": "build",
            "args": [
                "rustc",
            ]
        },
        {
            "taskName": "release_build",
            "args": [
                "rustc",
                "--release"
            ]
        }
    ]
}